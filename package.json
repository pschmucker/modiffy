{
  "name": "modiffy",
  "version": "1.0.2",
  "description": "Provides a React component to easily view all meaningful differences between 2 objects",
  "source": "src/index.tsx",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/pschmucker/modiffy.git"
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --build tsconfig.esm.json",
    "build:cjs": "tsc --build tsconfig.cjs.json",
    "link": "rm -rf ./dist && npm rm modiffy -g && parcel build && npm link",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "test": "jest"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "react",
    "typescript",
    "diff",
    "objects",
    "visual",
    "user-friendly",
    "meaningful"
  ],
  "author": "Philippe Schmucker",
  "license": "MIT",
  "devDependencies": {
    "@parcel/packager-ts": "^2.8.3",
    "@parcel/transformer-sass": "^2.8.3",
    "@parcel/transformer-typescript-types": "^2.8.3",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.4.0",
    "@types/json-diff": "^0.9.1",
    "@types/react": "^18.0.27",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.4.1",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^29.4.1",
    "parcel": "^2.8.3",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "json-diff": "^1.0.0"
  }
}
